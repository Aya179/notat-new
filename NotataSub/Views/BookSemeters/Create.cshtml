@model NotataSub.Models.BookSemeter

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}

<script src="https://cdnjs.cloudflare.com/ajax/libs/chosen/1.8.7/chosen.jquery.min.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/chosen/1.4.2/chosen.min.css" />

<style>
    .create-page {
        display: unset;
    }
</style>
@section CreatePage
{
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="col-5">
            <div class="form-group">
                <label asp-for="semeterTitel" class="control-label"></label>
                <input asp-for="semeterTitel" class="form-control" />
                <span asp-validation-for="semeterTitel" class="text-danger"></span>
            </div>
            </div>
        <div class="col-5">
            <div class="form-group">
                <label asp-for="pagenum" class="control-label"></label>
                <input asp-for="pagenum" class="form-control" />
                <span asp-validation-for="pagenum" class="text-danger"></span>
            </div>
            </div>
        <div class="col-5">
            <div class="form-group">
                <label asp-for="bookTitle" class="control-label"></label>
                <select asp-for="bookTitle" class="form-control" asp-items="ViewBag.bookId"></select>
            </div>
            </div>
            
        </form>
   }
@section Scripts {
    <!--sweetalert-->

    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/chosen/1.8.7/chosen.jquery.min.js" integrity="sha512-rMGGF4wg1R73ehtnxXBt5mbUfN9JUJwbk21KMlnLZDJh7BkPmeovBuddZCENJddHYYMkCh9hPFnPmS9sspki8g==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-te/1.4.0/jquery-te.min.js" integrity="sha512-813LH2NdwwzXnVfsmzSuAyyit5bRFdh997hN9Vzm0cdx3LdZV7TZNNb2Ag0dgJPD3J1Xn1Alg2YW70id+RtLrw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script>
        function show() {
           
        }
    </script>
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}



