@model NotataSub.Models.Studyyear

@{ ViewData["Title"] = "Edit"; }


<style>
    .edit-page {
        display: unset;
    }
</style>

@section EditPage
{
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <input type="hidden" asp-for="Id" />
    <div class="col-5">

        <div class="form-group">
            <label asp-for="Yearname" class="control-label"></label>
            <input maxlength="10" asp-for="Yearname" class="form-control" />
            <span asp-validation-for="Yearname" class="text-danger"></span>
        </div>
    </div>
    <div class="col-5">

        <div class="form-group">
            <label asp-for="Deptid" class="control-label"></label>
            <select asp-for="Deptid" class="form-control" asp-items="ViewBag.Deptid"></select>
            <span asp-validation-for="Deptid" class="text-danger"></span>
        </div>
        </div>


}



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
