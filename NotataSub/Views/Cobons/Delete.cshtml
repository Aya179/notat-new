@model NotataSub.Models.Cobon

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}

<style>
    .delete-page {
        display: unset;
    }
</style>

@section DeletePage
{
<table class="table">


     <tr>
            <th class="w-25">
                @Html.DisplayNameFor(model => model.CobonValue)
            </th>
            <td>
                @Html.DisplayFor(model => model.CobonValue)
            </td>
          
        </tr>
        <tr>


            <th class="w-25">
                العدد:
            </th>
            <td>
                @ViewBag.count
            </td>
        </tr>

         <tr>
            <th class="w-25">
                @Html.DisplayNameFor(model => model.ReleaseDate)
            </th>
            <td>
                @Html.DisplayFor(model => model.ReleaseDate)
            </td>
        </tr>


</table>
   <div class="mt-2 mr-4">
        <form asp-action="Delete">
            <input type="hidden" asp-for="CobonValue" />
            <button type="submit" class="btn btn-outline-danger" />
            <i class="far fa-trash-alt"></i>
            حذف
        </form>
    </div>


   } 
    
  
