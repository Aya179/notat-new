@model NotataSub.Models.Studyyear

@{ ViewData["Title"] = "Create"; }

<style>
    .create-page {
        display: unset;
    }
</style>

@section CreatePage
{
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="col-5">

        <div class="form-group">
            <label asp-for="Yearname" class="control-label"></label>
            <input maxlength="10" asp-for="Yearname" class="form-control" />
            <span asp-validation-for="Yearname" class="text-danger"></span>
        </div>
    </div>
    <div class="col-5">

        <div class="form-group">
            <label asp-for="Deptid" class="control-label"></label>
            <select asp-for="Deptid" class="form-control" asp-items="ViewBag.Deptid"></select>
        </div>
        </div>

}


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
