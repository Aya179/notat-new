@model NotataSub.Models.University


@{ ViewData["Title"] = "Detiles"; }

<style>
    .detiles-page {
        display: unset;
    }
</style>


@section DetilesPage
{
    <table class="table">

        <tr>
            <th class="w-25">
                @Html.DisplayNameFor(model => model.UniversityName)
            </th>
            <td>
                @Html.DisplayFor(model => model.UniversityName)
            </td>
        </tr>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.City)
            </th>
            <td>
                @Html.DisplayFor(model => model.City)
            </td>
        </tr>
      
    </table>


    <div class="mt-2 mr-4">
        <form>
            <button type="submit" class="btn btn-outline-primary" asp-action="Edit" asp-route-id="@Model.UniversityId">
                <i class="far fa-edit"></i>
                تعديل
            </button>
        </form>
       
    </div>
}